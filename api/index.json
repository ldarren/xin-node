{ 
    "deps":{
		"util": "./util",
		"ums": "./ums",
		"config": "./config",
		"s3": "./s3",
		"web": ["context", "webServer"]
    },
    "routes":{
        "ERR/#msg":[
            []
        ],
        "END/#msg":[
            []
        ],
		"/s3/list":[
			["s3", "list", "emails"],
			["util", "output", "emails"],
			[]
		],
		"/s3/read/:key":[
			["s3", "read", "email"],
			["util", "output", "email"],
			[]
		],
		"/config/mailbox/create": [
			["web", "getBody", "req", "body"],
			["config", "save", "body"],
			[]
		],
		"/config/mailbox/list": [
			["config", "list"],
			[]
		],
		"/config/mailbox": [
			["util", "branch", "req", [{
				"POST": "config/mailbox/create",
				"GET": "config/mailbox/list"
			}]],
			[]
		],
        "/say/hello":[
            ["util", "sayNow"],
			[]
        ]
    }
}
